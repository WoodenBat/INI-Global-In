게시글 이미지 테이블
CREATE TABLE board_image (
    id NUMBER GENERATED BY DEFAULT ON NULL AS IDENTITY PRIMARY KEY,
    board_id NUMBER NOT NULL,
    image_path VARCHAR2(500) NOT NULL,
    original_name VARCHAR2(255),
    upload_date DATE DEFAULT SYSDATE,
    
    CONSTRAINT fk_board_image_board
        FOREIGN KEY (board_id)
        REFERENCES INI_BOARD(BOARD_ID)
        ON DELETE CASCADE
);

게시글 시퀀스
CREATE SEQUENCE INI_BOARD_SEQ START WITH 1 INCREMENT BY 1 NOCACHE;

게시글 이미지 시퀀스
CREATE SEQUENCE BOARD_IMAGE_SEQ START WITH 1 INCREMENT BY 1 NOCACHE;

신고 시퀀스
CREATE SEQUENCE BOARD_REPORT_SEQ START WITH 1 INCREMENT BY 1 NOCACHE;
